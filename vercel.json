{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/genai.js": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/server/index.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  }
}
